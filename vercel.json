{
  "rewrites": [

    {
      "source": "/",
      "destination": "/index.html" 
      
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/script.js"
    },
    
    {
      "source": "/(.*)\\.html",
      "destination": "/$1.html"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
